{
    "Values":{
        "known":{
            "C" :"_NPC.known = 1",
            "VT": "Plain",
            "CV":{
                "Values":{
                    "hasNickName":{
                        "C" :"_NPC.nameNick != \"\"",
                        "VT": "Interpolate",
                        "K":"{_NPC.nameFirst} {_NPC.nameLast}\n<i>{_NPC.nameNick}</i>"
                    },
                    "noNick":{
                        "VT": "Interpolate",
                        "K":"{_NPC.nameFirst} {_NPC.nameLast}"
                    }
                }
            }
        },
        "unknown":{
            "CV":{
                "Values":{
                    "0": {"C":"_NPC.age < 18", "V": "A kid"},
                    "18f": {"C":"(_NPC.age >= 18) AND (_NPC.genderVisible = \"f\")", "V": "A woman"},
                    "18m": {"C":"(_NPC.age >= 18) AND (_NPC.genderVisible = \"m\")", "V": "A man"}
                }
            }
        }
    }
}