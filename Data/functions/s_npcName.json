{
    "Values":{
        "1_pc":{
            "Condition" :"_NPC.ID = \"PC\"",
            "ValueType": "Interpolate",
            "Key":"You\n({_NPC.NameFirst} {_NPC.NameLast})"
        },
        "5_known":{
            "Condition" :"_NPC.Social.KnowsName = true",
            "ValueType": "Plain",
            "ConditionalValue":{
                "Values":{
                    "5_hasNickName":{
                        "Condition" :"_NPC.NameNick != \"\"",
                        "ValueType": "Interpolate",
                        "Key":"{_NPC.NameFirst} {_NPC.NameLast}\n<i>{_NPC.nameNick}</i>"
                    },
                    "6_noNick":{
                        "ValueType": "Interpolate",
                        "Key":"{_NPC.NameFirst} {_NPC.NameLast}"
                    }
                }
            }
        },
        "7_unknown":{
            "ConditionalValue":{
                "Values":{
                    "0": {"Condition":"_NPC.Age < 18", "Value": "A kid"},
                    "18f": {"Condition":"(_NPC.Age >= 18) AND (_NPC.Body.GenderVisible = \"f\")", "Value": "A woman"},
                    "18m": {"Condition":"(_NPC.Age >= 18) AND (_NPC.Body.GenderVisible = \"m\")", "Value": "A man"}
                }
            }
        }
    }
}